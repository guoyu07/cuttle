{
    "name": "overtrue/cuttle",
    "description": "A multi-module log wrapper.",
    "type": "library",
    "keywords": ["log", "multi logger", "monolog"],
    "require": {
        "php": ">=7.0",
        "monolog/monolog": "~1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.2",
        "mockery/mockery": "1.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Overtrue\\Cuttle\\": "src"
        },
        "files": ["src/Helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Overtrue\\Cuttle\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "overtrue",
            "email": "i@overtrue.me"
        }
    ]
}
